<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="464.0" prefWidth="471.0">
         <children>
            <Label fx:id="usernameLbl" layoutX="60.0" layoutY="183.0" text="%username" />
            <Label fx:id="passwordLbl" layoutX="60.0" layoutY="236.0" text="%password" />
            <TextField fx:id="usernameTxt" layoutX="170.0" layoutY="179.0" />
            <PasswordField fx:id="passwordTxt" layoutX="170.0" layoutY="232.0" />
            <Button fx:id="submitBtn" layoutX="267.0" layoutY="287.0" mnemonicParsing="false" onAction="#submitBtnClicked" text="%login" />
            <Label fx:id="companyNameLbl" layoutX="34.0" layoutY="27.0" text="%companyName">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <Label fx:id="locationLbl" layoutX="71.0" layoutY="62.0" text="%location" />
            <Label fx:id="loginErrorLbl" layoutX="170.0" layoutY="126.0" prefHeight="35.0" prefWidth="225.0" text="\%loginError" textFill="RED" visible="false" wrapText="true" />
            <Button fx:id="exitProgramBtn" cancelButton="true" layoutX="343.0" layoutY="411.0" mnemonicParsing="false" onAction="#exitProgramBtnClicked" text="Exit Program" />
         </children>
      </Pane>
   </children>
</GridPane>
